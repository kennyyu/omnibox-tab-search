{
  "manifest_version": 2,

  "name": "Tab Search",
  "description": "Search your tabs and jump to them!",
  "version": "0.0.1",

  "background": {
    "scripts": [ "search.js" ]
  },

  "icons": {
    "16": "icons/16-search.png",
    "48": "icons/48-search.png",
    "128": "icons/128-search.png"
  },

  "permissions": [
    "tabs"
  ],

  "omnibox": {
    "keyword": "ts"
  }
}